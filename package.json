{
  "name": "store-front-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"tsc --watch\" \" nodemon dist/server.js\" ",
    "migrate": "db-migrate up --config database.json",
    "migrate:rollback": "db-migrate down",
    "db-create": "dotenv -e .env -- cross-var db-migrate db:create %DB_NAME%",
    "db-drop": "dotenv -e .env -- cross-var db-migrate db:drop %DB_NAME%",
    "build": "tsc"
  },
